#include<stdio.h>
#include<string.h>
void main()
{
    char a[100];
    printf("Enter password:");
    scanf("%s",a);
    int l=strlen(a),i;
    int up=0,lw=0,dg=0,sp=0;
    if(l>8)
    {
        for(i=0;i<l;i++)
        {
            if(isupper(a[i]))
            up++;
            else if(islower(a[i]))
            lw++;
            else if(isdigit(a[i]))
            dg++;
            else
            sp++;
        }
        if(up>0 && lw>0 && dg>0 && sp>0)
        printf("STRONG PASSWORD");
        else
        printf("WEAK PASSWORD");
    }
    else
    printf("enter atleast 8");
}
