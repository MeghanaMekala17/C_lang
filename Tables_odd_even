#include<stdio.h>
void main()
{
    int n,i;
    printf("enter any number");
    scanf("%d",&n);
    if(n%2==0)
    {
        
    for(i=2;i<=10;i++)
    {
        printf("%d*%d=%d",i,n,i*n);
        printf("/n");
    }
    }
    else
    {
        for(i=1;i<=15;i++)
        {
            printf("%d*%d=%d",i,n,i*n);
            printf("/n");
        }
    }
}
